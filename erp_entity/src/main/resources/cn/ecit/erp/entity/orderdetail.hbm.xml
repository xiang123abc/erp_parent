<hibernate-mapping>
    <class name="cn.ecit.erp.entity.Orderdetail" table="orderdetail" >
        <id name="uuid"> 
         <generator class="assigned"/> 
       </id> 
       <property name="orderdetailcode" />
		<property name="code1" /> 
        <property name="code2" />
        <property name="goodscode" /> 
        <property name="goodsname" />
        <property name="goodstype" /> 
        <property name="price" /> 
        <property name="num" /> 
        <property name="height" /> 
        <property name="weight" /> 
        <property name="area" /> 
        <property name="note" /> 
        <property name="money" /> 
        <property name="endtime" /> 
        <property name="ender" /> 
        <property name="storecode" /> 
        <property name="state" /> 
        <property name="orderscode" />
