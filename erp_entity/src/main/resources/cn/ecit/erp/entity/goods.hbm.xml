<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="cn.ecit.erp.entity.Goods" table="goods" >
        <id name="uuid"> 
         <generator class="identity"/> 
        </id> 
		<property name="goodscode" />
        <property name="name" /> 
        <property name="origin" /> 
        <property name="producer" /> 
        <property name="unit" /> 
        <property name="inprice" /> 
        <property name="outprice" /> 
<!--         <property name="goodstypeuuid" />  -->
       <!-- <many-to-one name="goodstype" column="goodstypeuuid" class="cn.ecit.erp.entity.Goodstype"></many-to-one> -->
 
    </class>
</hibernate-mapping>
